{
  "apiKey": "your-codegpt-api-key-here",
  "model": "gpt-4",
  "temperature": 0.7,
  "maxTokens": 2048,
  "contextWindow": 4096,
  "pinnacleIntegration": {
    "enabled": true,
    "endpoint": "http://localhost:8000/api/codegpt",
    "autoSync": true
  },
  "features": {
    "autoComplete": true,
    "codeExplanation": true,
    "codeGeneration": true,
    "codeReview": true,
    "documentation": true,
    "refactoring": true,
    "testing": true
  },
  "exclude": [
    "**/node_modules/**",
    "**/venv/**",
    "**/__pycache__/**",
    "**/.git/**",
    "**/*.min.js",
    "**/*.min.css"
  ]
}
